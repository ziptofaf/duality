$(document).ready ->
  $('.targetDiv').hide()
  $('.single').click ->
    $('#extended'+$(this).attr('target')).slideToggle(200)
    $('#button'+$(this).attr('target')).attr("src", <%= "\"#{image_path("expand.png")}\"" %>) and 
    flag=1 if $('#button'+$(this).attr('target')).attr("src")==<%= "\"#{image_path("collapse.png")}\"" %>
    $('#button'+$(this).attr('target')).attr("src", <%= "\"#{image_path("collapse.png")}\"" %>) if flag!=1
    flag=0
  return
return
